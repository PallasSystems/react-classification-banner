{
  "name": "@pallassystems/react-security-classification-banner",
  "version": "0.9.0",
  "description": "A library to generate  UK Security Classification Banners based on UK Government Guidance.",
  "homepage": "https://pallassystems.github.io/react-security-classification-banner",
  "keywords": [
    "react",
    "security",
    "banner",
    "classification",
    "security classification banner"
  ],
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "private": false,
  "dependencies": {
    "bootstrap": ">=5.3.0",
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  },
  "peerDependencies": {
    "@types/react": ">=18.0.0",
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0",
    "typescript": "^4.9.5"
  },
  "overrides": {
    "react-refresh": "0.14.0",
    "svgo": "3.0.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": ">=7.21.11",
    "@babel/preset-react": ">=7.22.5",
    "@babel/preset-typescript": "^7.23.2",
    "@storybook/addon-actions": ">=7.4.6",
    "@storybook/addon-essentials": ">=7.4.6",
    "@storybook/addon-interactions": ">=7.4.6",
    "@storybook/addon-links": ">=7.4.6",
    "@storybook/node-logger": ">=7.4.6",
    "@storybook/preset-create-react-app": ">=7.4.6",
    "@storybook/react": ">=7.4.6",
    "@storybook/react-webpack5": ">=7.4.6",
    "@storybook/testing-library": ">=0.2.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^27.5.2",
    "@types/node": ">=16.18.11",
    "@types/react": ">=18.0.26",
    "@types/react-router-bootstrap": ">=0.26.0",
    "eslint-config-prettier": ">=9.0.0",
    "eslint-plugin-prettier": ">=5.0.0",
    "gh-pages": ">=6.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "prop-types": ">=15.8.1",
    "storybook": ">=7.4.6",
    "stylelint": ">=16.0.0",
    "stylelint-config-standard": ">=35.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": ">=4.9.5"
  },
  "scripts": {
    "clean": "rm -rf ./dist ./coverage eslint_report.json stylelint_report.json junit.xml",
    "build": "npm run clean && tsc",
    "test": "jest",
    "storybook": "storybook dev -p 6006 -s public",
    "build-storybook": "storybook build -s public",
    "analyse": "source-map-explorer 'build/static/js/*.js'",
    "eslint": "eslint --ext .ts --ext .tsx --output-file eslint_report.json --format json src",
    "stylelint": "stylelint \"./src/**/*.css\" --formatter json --output-file stylelint_report.json",
    "lint": "npm run eslint && npm run stylelint",
    "postpublish": "gh-pages -d docs/dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PallasSystems/react-security-classification-banner.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/index.js"
      },
      "default": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  }
}
