{
  "name": "@pallassystems/react-security-classification-banner",
  "version": "1.1.8",
  "description": "A library to generate  UK Security Classification Banners based on UK Government Guidance.",
  "homepage": "https://rscb.pallas.uk",
  "keywords": [
    "react",
    "security",
    "banner",
    "classification",
    "security classification banner"
  ],
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "private": false,
  "dependencies": {
    "bootstrap": ">=5.3.0",
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0",
    "typescript": ">=5.4.5"
  },
  "peerDependencies": {
    "@types/react": ">=18.0.0",
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0",
    "typescript": "^4.9.5"
  },
  "overrides": {
    "react-refresh": "0.14.0",
    "svgo": "3.0.2"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.5.0",
    "@storybook/addon-essentials": "^8.1.10",
    "@storybook/addon-interactions": "^8.1.10",
    "@storybook/addon-links": "^8.1.10",
    "@storybook/addon-onboarding": "^8.1.10",
    "@storybook/blocks": "^8.1.10",
    "@storybook/react": "^8.1.10",
    "@storybook/react-vite": "^8.1.10",
    "@storybook/test": "^8.1.10",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.12",
    "@types/node": ">=16.18.11",
    "@types/react": ">=18.0.26",
    "@types/react-router-bootstrap": ">=0.26.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint-config-prettier": ">=9.0.0",
    "eslint-plugin-prettier": ">=5.0.0",
    "gh-pages": "^6.1.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "prop-types": ">=15.8.1",
    "storybook": "^8.1.10",
    "stylelint": ">=15.7.0",
    "stylelint-config-standard": ">=33.0.0",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "vite": "^5.2.9"
  },
  "scripts": {
    "clean": "rm -rf ./dist ./coverage eslint_report.json stylelint_report.json junit.xml",
    "build": "npm run clean && tsc",
    "test": "jest",
    "storybook": "storybook dev -p 6006 -s public",
    "build-storybook": "storybook build -s public",
    "analyse": "source-map-explorer 'build/static/js/*.js'",
    "eslint": "eslint --ext .ts --ext .tsx --output-file eslint_report.json --format json src",
    "stylelint": "stylelint \"./src/**/*.css\" --formatter json --output-file stylelint_report.json",
    "lint": "npm run eslint && npm run stylelint",
    "postpublish": "gh-pages -d docs/dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PallasSystems/react-security-classification-banner.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/index.js"
      },
      "default": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  }
}
